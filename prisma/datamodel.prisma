type Poll {
  id: ID! @id
  createdAt: DateTime! @createdAt
  description: String!
  url: String!
  postedBy: User
  votes: [Vote!]!
}

type User {
  id: ID! @id
  name: String!
  email: String! @unique
  password: String!
  polls: [Poll!]!
  votes: [Vote!]!
}

type Vote {
  id: ID! @id
  poll: Poll!
  user: User!
  dificulty: Float!
}